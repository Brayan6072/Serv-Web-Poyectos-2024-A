<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MdReportes</servlet-name>
        <servlet-class>ModeloReporte.MdReportes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidarUs</servlet-name>
        <servlet-class>ModeloLogin.ValidarUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ActualizarU</servlet-name>
        <servlet-class>ModeloLogin.ActualizarU</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertarU</servlet-name>
        <servlet-class>ModeloLogin.InsertarU</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarU</servlet-name>
        <servlet-class>ModeloLogin.EliminarU</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MdReportes</servlet-name>
        <url-pattern>/MdReportes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidarUs</servlet-name>
        <url-pattern>/ValidarUs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ActualizarU</servlet-name>
        <url-pattern>/ActualizarU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertarU</servlet-name>
        <url-pattern>/InsertarU</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarU</servlet-name>
        <url-pattern>/EliminarU</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
